<% content_for :breadcrumbs do %>
  <%= breadcrumbs do |list| %>
    <% list.link "People", "/search" %>
    <% list.link "New #{@role.humanize}", "/people/new?role=caller" %>
  <% end %>
<% end %>
<% content_for :page_head do %>
  <h1 class="text-2xl font-semibold text-gray-900"><%= "New #{@role.humanize}" %></h1>
<% end %>
<%= render "form", person: @person, role: @role %>
