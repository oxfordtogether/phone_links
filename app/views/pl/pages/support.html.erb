<% content_for :breadcrumbs do %>
  <%= breadcrumbs home_path: pl_path(@current_pod_leader) do |list| %>
    <% list.link "Support", pl_support_path(@current_pod_leader) %>
  <% end %>
<% end %>
<% content_for :page_head do %>
  <h1 class="pb-8 text-2xl font-semibold text-gray-900">
    Support
  </h1>
<% end %>
<div class="flex flex-col">
  <div class="flex flex-col items-center">
    <div class="p-1 font-semibold	text-lg text-gray-900">Need some help or have a request?</div>
    <div class="p-1 text-gray-900">Please get in touch via <%= mail_to "support@oxfordcodelab.com" %></div>
  </div>
</div>
