<% content_for :breadcrumbs do %>
  <%= breadcrumbs do |list| %>
    <% list.link "Callees", pl_callees_path(@current_pod_leader) %>
    <% list.link @person.name, pl_callee_path(@current_pod_leader, @callee) %>
  <% end %>
<% end %>
<% content_for :page_head do %>
  <%= render partial: "show_page_head" %>
<% end %>
<div class="max-w-3xl mx-auto sm:px-6 lg:max-w-7xl ">
  <section aria-labelledby="timeline-title">
    <div class="bg-white px-4 py-5 shadow sm:rounded-lg sm:px-6">
      <% if false %><%= events_timeline(@events) %><% end %>
    </div>
  </section>
</div>
