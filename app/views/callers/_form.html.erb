<%= tailwind_form_with(model: @caller, local: true) do |form| %>
  <%= form.section do %>
    <%= form.hidden_field :person_id, value: @caller.person.id %>
    <%= form.date_field :start_date, width: :third %>
  <% end %>
  <%= form.button_section do %>
    <%= form.cancel '#' %>
    <%= form.submit %>
  <% end %>
<% end %>
