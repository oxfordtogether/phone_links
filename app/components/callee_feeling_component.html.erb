<% case @feeling %>
<% when :awful %>
<%= icon("openmoji-crying-face", class: "w-8 h-8") %>
<% when :bad %>
<%= icon("openmoji-slightly-frowning-face", class: "w-8 h-8") %>
<% when :neutral %>
<%= icon("openmoji-neutral-face", class: "w-8 h-8") %>
<% when :good %>
<%= icon("openmoji-slightly-smiling-face", class: "w-8 h-8") %>
<% when :great %>
<%= icon("openmoji-grinning-face", class: "w-8 h-8") %>
<% else %>
  <%= icon("question", class: "w-8 h-8") %>
<% end %>
